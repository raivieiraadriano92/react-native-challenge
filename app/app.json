{
  "expo": {
    "name": "Hacker News",
    "slug": "hacker-news",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6600"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ]
      },
      "bundleIdentifier": "com.raivieiraadriano92.hackernews"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FF6600"
      },
      "package": "com.raivieiraadriano92.hackernews"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
